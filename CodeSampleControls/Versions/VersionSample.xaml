<UserControl x:Class="WpfCsSample.CodeSampleControls.Versions.VersionSample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignWidth="325" d:DesignHeight="273">
    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <Button Content="Display Versions" Click="Button_Click"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="SDK Version:"/>
            <Label Name="labelSdkVersion" Content="?"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Binding Version:"/>
            <Label Name="labelBindingVersion" Content="?"/>
        </StackPanel>
    </StackPanel>
</UserControl>
